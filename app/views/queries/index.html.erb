<p id="notice"><%= notice %></p>

<h1>Queries</h1>

<table>
  <thead>
    <tr>
    <th>Username</th>
    <th>Gender ID</th>
      <th>Ethnicity ID</th>
      <th>Consent</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @queries.each do |query| %>
      <tr>
        <td><%= query.username %></td>
        <td><%= query.gender_ids %></td>
        <td><%= query.ethnicity_ids %></td>
        <td><%= query.consent %></td>
        <td><%= link_to 'Show', query %></td>
        <td><%= link_to 'Edit', edit_query_path(query) %></td>
        <td><%= link_to 'Destroy', query, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Query', new_query_path %>
